Cell[CellGroupData[{

Cell["Compute the Fourier Spectrum of the Source", "Section",
 CellChangeTimes->{{3.927269420170882*^9, 
  3.927269432802559*^9}},ExpressionUUID->"01184e1b-c7d9-443f-8742-\
2a2ffc843891"],

Cell["\<\
First, define a set of helper functions. These depend on the (transverse) \
momentum p_ and the coordinate \[Alpha]_ on the Freezeout surface. Eventually \
we wish to integrate over the Freezeout surface. The dependence on p_ can be \
substituted by explicit evaluation on a (nicely resolved) grid in momentum \
space. The dependence on \[Alpha]_ must remain symbolic, since we wish to \
numerically integrate over \[Alpha]_ using NIntegrate. \
\>", "Text",
 CellFrame->{{0, 0}, {0, 0}},
 CellChangeTimes->{{3.927433210557094*^9, 3.927433457136797*^9}},
 Background->RGBColor[
  1, 0.9, 0.8],ExpressionUUID->"be4c9663-5775-4a83-b60a-449d3054d840"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"\[Omega]", "[", "p_", "]"}], "=", 
    RowBox[{"Sqrt", "[", 
     RowBox[{
      RowBox[{"p", "^", "2"}], "+", 
      RowBox[{"m\[Pi]", "^", "2"}]}], "]"}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{"in", " ", "GeV", " ", 
    RowBox[{"(", 
     RowBox[{"p", " ", "in", " ", "GeV"}], ")"}]}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"J0rp", "[", 
     RowBox[{"\[Alpha]_", ",", "p_"}], "]"}], "=", 
    RowBox[{"BesselJ", "[", 
     RowBox[{"0", ",", 
      RowBox[{
       RowBox[{"r", "[", "\[Alpha]", "]"}], "*", "p", "*", "GeVtoIfm"}]}], 
     "]"}]}], ";"}], 
  RowBox[{"(*", "dimensionless", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"J1rp", "[", 
     RowBox[{"\[Alpha]_", ",", "p_"}], "]"}], "=", 
    RowBox[{"BesselJ", "[", 
     RowBox[{"1", ",", 
      RowBox[{
       RowBox[{"r", "[", "\[Alpha]", "]"}], "*", "p", "*", "GeVtoIfm"}]}], 
     "]"}]}], ";"}], 
  RowBox[{"(*", "dimensionless", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Y0tw", "[", 
     RowBox[{"\[Alpha]_", ",", "p_"}], "]"}], "=", 
    RowBox[{"BesselY", "[", 
     RowBox[{"0", ",", 
      RowBox[{
       RowBox[{"\[Tau]", "[", "\[Alpha]", "]"}], "*", 
       RowBox[{"\[Omega]", "[", "p", "]"}], "*", "GeVtoIfm"}]}], "]"}]}], 
   ";"}], 
  RowBox[{"(*", "dimensionless", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Y1tw", "[", 
     RowBox[{"\[Alpha]_", ",", "p_"}], "]"}], "=", 
    RowBox[{"BesselY", "[", 
     RowBox[{"1", ",", 
      RowBox[{
       RowBox[{"\[Tau]", "[", "\[Alpha]", "]"}], "*", 
       RowBox[{"\[Omega]", "[", "p", "]"}], "*", "GeVtoIfm"}]}], "]"}]}], 
   ";"}], 
  RowBox[{"(*", "dimensionless", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"J0tw", "[", 
     RowBox[{"\[Alpha]_", ",", "p_"}], "]"}], "=", 
    RowBox[{"BesselJ", "[", 
     RowBox[{"0", ",", 
      RowBox[{
       RowBox[{"\[Tau]", "[", "\[Alpha]", "]"}], "*", 
       RowBox[{"\[Omega]", "[", "p", "]"}], "*", "GeVtoIfm"}]}], "]"}]}], 
   ";"}], 
  RowBox[{"(*", "dimensionless", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"J1tw", "[", 
     RowBox[{"\[Alpha]_", ",", "p_"}], "]"}], "=", 
    RowBox[{"BesselJ", "[", 
     RowBox[{"1", ",", 
      RowBox[{
       RowBox[{"\[Tau]", "[", "\[Alpha]", "]"}], "*", 
       RowBox[{"\[Omega]", "[", "p", "]"}], "*", "GeVtoIfm"}]}], "]"}]}], 
   ";"}], 
  RowBox[{"(*", "dimensionless", "*)"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"H1", "[", 
     RowBox[{"\[Alpha]_", ",", "p_"}], "]"}], "=", 
    RowBox[{
     RowBox[{"J0rp", "[", 
      RowBox[{"\[Alpha]", ",", "p"}], "]"}], "*", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"Y0tw", "[", 
         RowBox[{"\[Alpha]", ",", "p"}], "]"}]}], "+", 
       RowBox[{"I", "*", 
        RowBox[{"J0tw", "[", 
         RowBox[{"\[Alpha]", ",", "p"}], "]"}]}]}], ")"}]}]}], ";"}], 
  RowBox[{"(*", "dimensionless", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"H2", "[", 
     RowBox[{"\[Alpha]_", ",", "p_"}], "]"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"D\[Tau]", "[", "\[Alpha]", "]"}], "*", "fmtoIGeV", "*", "p", 
      "*", 
      RowBox[{"J1rp", "[", 
       RowBox[{"\[Alpha]", ",", "p"}], "]"}], "*", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Y0tw", "[", 
          RowBox[{"\[Alpha]", ",", "p"}], "]"}]}], "+", 
        RowBox[{"I", "*", 
         RowBox[{"J0tw", "[", 
          RowBox[{"\[Alpha]", ",", "p"}], "]"}]}]}], ")"}]}], "+", 
     RowBox[{"(*", "dimensionless", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Dr", "[", "\[Alpha]", "]"}], "*", "fmtoIGeV", "*", 
      RowBox[{"J0rp", "[", 
       RowBox[{"\[Alpha]", ",", "p"}], "]"}], "*", 
      RowBox[{"\[Omega]", "[", "p", "]"}], "*", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Y1tw", "[", 
          RowBox[{"\[Alpha]", ",", "p"}], "]"}]}], "+", 
        RowBox[{"I", "*", 
         RowBox[{"J1tw", "[", 
          RowBox[{"\[Alpha]", ",", "p"}], "]"}]}]}], ")"}]}]}]}], ";"}], 
  RowBox[{"(*", "dimensionless", "*)"}]}]}], "Input",
 InitializationCell->True,
