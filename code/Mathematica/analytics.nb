Cell[BoxData[
 RowBox[{
  RowBox[{"\[Epsilon]", "[", "\[Chi]2", "]"}], "=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"\[Chi]2", "+", 
     RowBox[{"\[Mu]", "^", "2"}]}], ")"}], "*", 
